{
  "expo": {
    "name": "expo-firebase-instagram",
    "description": "expo-firebase-instagram",
    "slug": "expo-firebase-instagram",
    "privacy": "unlisted",
    "sdkVersion": "29.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
