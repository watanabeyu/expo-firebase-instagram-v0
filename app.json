    "android": {
      "versionCode": 1,
      "icon": "./assets/images/icon.png",
      "package": "xxxxxxxxxxxxxxx",
      "permissions": [
        "CAMERA",
        "AUDIO_RECORDING",
        "NOTIFICATIONS",
        "READ_INTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
